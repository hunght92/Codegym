   "source": [
    "import tkinter as tk\n",
    "from tkinter import ttk\n",
    "\n",
    "window = tk.Tk()\n",
    "window.title(\"Weight Conversion\")\n",
    "window.geometry(\"600x500\")\n",
    "\n",
    "text = ttk.Label(\n",
    "    window,\n",
    "    text = \"Enter the weight in Kg\"\n",
    ")\n",
    "text.grid()\n",
    "\n",
    "\n",
    "\n",
    "tk.mainloop()"
   ]
