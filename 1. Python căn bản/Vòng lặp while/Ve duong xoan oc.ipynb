   "source": [
    "import turtle\n",
    "h = turtle.Turtle()\n",
    "d = 0.1\n",
    "\n",
    "while True:\n",
    "    h.forward(d)\n",
    "    h.left(10)\n",
    "    d += 0.1\n",
    "    if h.distance(0, 0) >= 300:\n",
    "        break\n",
    "        \n",
    "turtle.done()"
   ]
